#include <iostream>
using namespace std ;

int main () {
	int angka_awal , angka_akhir ;
	cout << "Masukkan angka awal : " ;
	cin >> angka_awal ;
	cout << endl ;
	cout << "Masukkan angka akhir : " ;
	cin >> angka_akhir ;
	cout << endl ;
	
	for ( int i = angka_awal ; i <= angka_akhir ; i++) {
		if ( i % 2 == 1 ) {
			cout << i << " adalah angka ganjil " << endl ;
		} else {
			cout << i << " adalah angka genap " << endl ;
		}
	}
}
